{
  "name": "vue-todo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "3.6.5",
    "vue": "2.6.11",
    "vuetify": "2.2.29"
  },
  "devDependencies": {
    "@babel/polyfill": "7.8.7",
    "@vue/cli-plugin-babel": "4.3.1",
    "@vue/cli-plugin-eslint": "4.3.1",
    "@vue/cli-service": "4.3.1",
    "@vue/test-utils": "1.0.3",
    "axios": "0.19.2",
    "babel-eslint": "10.1.0",
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "6.2.2",
    "husky": "4.2.5",
    "imagemin-lint-staged": "0.4.0",
    "lint-staged": "10.2.4",
    "mocha": "7.1.2",
    "sass": "1.26.5",
    "sass-loader": "8.0.2",
    "uuid": "8.1.0",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-vuetify": "2.0.5",
    "vue-router": "3.2.0",
    "vue-template-compiler": "2.6.11",
    "vuetify-loader": "1.4.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{html,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  }
}
